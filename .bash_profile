alias git_lost='echo -e "gwi_or_create          **create repo** \ngwi_or_push           **push to github** \ngwi_or_clone               **clone repo**"'

alias gwi_or_create='echo -e "mkdir(makes folder) \ncd dir (enters directory) \ntouch file (creates new file) \ngit init (INITizalizes the git repositroy)"'


alias gwi_or_push='echo -e "git add -A          **Adds changes to working directory** \ngit commit -m '"message"'       **commits your files with your message to git)**        \nCreate a Repo on GitHub https://help.github.com/articles/create-a-repo/ \ngit remote push [url]   **pushes your work repository to github**"'

alias gwi_or_clone='echo -e "git clone [repo url]       **Clones into current directory** \ngit add -A           **Adds changes to working directory**          \ngit commit -m "message"       **commits your files with your message to git** \ngit branch [name]     **create a new branch**"'
